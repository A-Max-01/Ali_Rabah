@model IEnumerable<MVC_Music_Store.Models.Genre>

@{ 
    ViewBag.Title = "Store";
}

<h3> Browse Genres</h3>

<p>Select from @Model.Count()
    Genres:</p>
<ul>
    @foreach (var genre in Model)
    {
        
        <li>
            @Html.ActionLink(genre.Name,"Browse",new { genre = genre.Name})
        </li>
    }
</ul>